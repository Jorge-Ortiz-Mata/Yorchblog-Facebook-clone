<div id="<%= dom_id profile %>">
  <p>
    <strong>First name:</strong>
    <%= profile.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= profile.last_name %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= profile.location %>
  </p>

  <p>
    <strong>Genre:</strong>
    <%= profile.genre %>
  </p>

  <p>
    <strong>Telephone:</strong>
    <%= profile.telephone %>
  </p>

  <p>
    <strong>Date birth:</strong>
    <%= profile.date_birth %>
  </p>

  <p>
    <strong>Occupation:</strong>
    <%= profile.occupation %>
  </p>

  <p>
    <strong>User:</strong>
    <%= profile.user_id %>
  </p>

</div>
